   <talk_paragraph index="n394_academic_station-accepting" image="QN_Npc_Academic_jasmine.dds" itemindex="">
      <talk_answer link_index="n394_academic_station-accepting-a" mid="6"><![CDATA[<html>I don't have a heart.</html>]]></talk_answer>
