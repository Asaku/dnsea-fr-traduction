   <talk_paragraph index="n003_trader_dolis-accepting-d" image="" itemindex="">
      <talk_answer link_index="n003_trader_dolis-accepting-acceptted" mid="14"><![CDATA[<html><FONT color=#086171>Accept quest.</FONT></html>]]></talk_answer>
